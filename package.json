{
  "name": "lazy-builder",
  "description": "Repeatedly and lazily rebuild an immutable map of files",
  "version": "0.1.1",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "immutable": "^3.7.6",
    "lodash": "^4.7.0",
    "pair-table": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "pretty-hrtime": "^1.0.2",
    "sinon": "^1.17.3",
    "source-map-support": "^0.4.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "root": "true",
    "parser": "babel-eslint",
    "rules": {
      "new-cap": 0,
      "no-lone-blocks": 0
    }
  },
  "files": [
    "dist/lib"
  ],
  "jsnext:main": "src/lib/LazyBuilder.js",
  "license": "MIT",
  "main": "dist/lib/LazyBuilder.js",
  "repository": "tripjs/lazy-builder",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "buildwatch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "eslint . && npm run build",
    "test": "mocha dist/test",
    "testwatch": "cd dist && mocha --watch"
  }
}
